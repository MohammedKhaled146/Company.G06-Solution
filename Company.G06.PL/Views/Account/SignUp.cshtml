
@model SignUpViewModel
@{
    ViewData["Title"] = "SignUp";
    Layout = "~/Views/Shared/_AuthLayout.cshtml";
}


	<div class="main-agileinfo">
	<div asp-validation-summary="All">
	</div>
		<div class="agileits-top">
			<form asp-action="SignUp">
				<input class="text m-2"  asp-for="UserName" placeholder="User Name" required = "">
			<input class="text m-2" asp-for="FirstName" placeholder="First Name" required="">
			<input class="text m-2" asp-for="LastName" placeholder="Last Name" required="">
			<input class="text email m-2" asp-for="Email" placeholder="Email" required="">
			<input class="text email m-2" asp-for="ConfirmEmail" placeholder="Confirm Email" required="">
			<input class="text m-2" asp-for="Password" placeholder="Password" required="">
			<input class="text m-2" asp-for="ConfirmPassword" placeholder="Confirm Password" required="">
				<div class="wthree-text">
					<label class="anim">
						
						<span>I Agree To The Terms & Conditions</span>
					</label>
					<div class="clear"> </div>
				</div>
				<input type="submit" value="SIGNUP">
			</form>
			<p>Don't have an Account? <a asp-action="SignIn"> Login Now!</a></p>
		</div>
	</div>

